{
  "name": "next-redux-wrapper-future",
  "version": "1.0.0",
  "description": "Redux wrapper for Next.js",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "types": "es6/index.d.ts",
  "source": "src/index.tsx",
  "jsnext:main": "src/index.tsx",
  "main:src": "src/index.tsx",
  "scripts": {
    "test": "jest",
    "test:quick": "yarn test",
    "clean": "rimraf lib es6 types coverage",
    "build": "concurrently 'yarn:build:tsc:*'",
    "build:tsc:es5": "tsc",
    "build:tsc:es6": "tsc --project tsconfig.es6.json",
    "start": "concurrently 'yarn:start:tsc:*'",
    "start:tsc:es5": "yarn build:tsc:es5 --watch --preserveWatchOutput",
    "start:tsc:es6": "yarn build:tsc:es6 --watch --preserveWatchOutput"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-redux": "7.1.30",
    "@types/react-test-renderer": "17.0.1",
    "@types/webpack": "5.28.0",
    "concurrently": "6.4.0",
    "jest": "27.4.3",
    "next": "^14.0.2",
    "next-redux-wrapper-future-configs": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.7",
    "react-test-renderer": "17.0.2",
    "redux": "4.1.2",
    "redux-promise-middleware": "6.1.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.7",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "next": ">=14",
    "react": "*",
    "react-redux": "*"
  },
  "author": "Kirill Konshin",
  "repository": {
    "type": "git",
